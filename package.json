{
  "name": "bookly-api",
  "version": "1.0.0",
  "description": "api for bookly",
  "main": "index.js",
  "scripts": {
    "build": "babel ./ -d build --ignore node_modules/ --presets=env --source-maps",
    "prebuild": "rm -rf build/*",
    "start": "NODE_ENV=production node ./build/index.js",
    "prestart": "npm run build",
    "dev": "NODE_ENV=development babel-node index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BooklyTech/bookly-api.git"
  },
  "keywords": [
    "edtech",
    "book",
    "education",
    "learning"
  ],
  "author": "Bookly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BooklyTech/bookly-api/issues"
  },
  "homepage": "https://github.com/BooklyTech/bookly-api#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.2.13",
    "validator": "^10.7.1"
  }
}
